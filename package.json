{
  "name": "@vasturiano/flatten",
  "version": "1.1.1",
  "description": "Recursively flatten nested arrays with arbitrary levels",
  "type": "module",
  "unpkg": "dist/flatten.min.js",
  "main": "dist/flatten.mjs",
  "module": "dist/flatten.mjs",
  "types": "dist/flatten.d.ts",
  "exports": {
    "umd": "./dist/flatten.min.js",
    "default": "./dist/flatten.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/flatten.git"
  },
  "keywords": [
    "flatten",
    "array",
    "nested",
    "recursive"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/flatten/issues"
  },
  "homepage": "https://github.com/vasturiano/flatten",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0",
    "rollup-plugin-dts": "^5.1.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12"
  }
}

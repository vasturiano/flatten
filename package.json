{
  "name": "@vasturiano/flatten",
  "version": "1.0.2",
  "description": "Recursively flatten nested arrays with arbitrary levels",
  "unpkg": "dist/flatten.min.js",
  "main": "dist/flatten.common.js",
  "module": "dist/flatten.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/flatten.git"
  },
  "keywords": [
    "flatten",
    "array",
    "nested",
    "recursive"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/flatten/issues"
  },
  "homepage": "https://github.com/vasturiano/flatten",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/flatten.js -o dist/flatten.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
